{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Sendora alfa version",
  "version": "0.1.0",
  "identifier": "com.menu.emailsender",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "endora alfa version",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
      "macOS": {
    "entitlements": null,
    "exceptionDomain": "",
    "signingIdentity": null
  }

  },

  "plugins": {
    "fs": {
      "requireLiteralLeadingDot": false
    }
  }





}
